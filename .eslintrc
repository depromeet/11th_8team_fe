{
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "next", "prettier", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "prefer-const": "warn",
    "no-undef": "off",
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", ["parent", "sibling"], "index"],
        "newlines-between": "always"
      }
    ]
  }
}
